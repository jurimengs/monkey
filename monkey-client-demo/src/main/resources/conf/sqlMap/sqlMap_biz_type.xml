<?xml version="1.0" encoding="UTF-8" ?>
<sqlMap namespace="BizType">
    <sql id="findListByMenuId" isRead="true">
        <![CDATA[
            select * from t_biz_type
        ]]>
    </sql>

    <sql id="findListNewByMenuId" isRead="true">
        <![CDATA[
            select
                biz_id as id,
                menu_id,
                biz_desc as biz_type_name
            from
                t_biz_tree
            where
                type = :type and `status` = 0
        ]]>
    </sql>
</sqlMap>
